<%= form_for @song, method: "patch" do |f| %>
  <%= f.text_field :title %>
  <%= f.label :released %>
  <%= f.check_box :released %>
  <%= f.label :release_year %>
  <%= f.number_field :release_year %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.label :genre %>
  <%= f.text_field :genre %>
  <%= f.submit "Update Song" %>
<% end %>
