<h1>Index</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Artist Name</th>
      <th>Year</th>
      <th>Genre</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.release_year %></td>
        <td><%= song.genre %></td>
        <td>
          <%= link_to 'Delete', song, method: :delete %>
        </td>
      </tr>

    <% end %>
  </tbody>
</table>
